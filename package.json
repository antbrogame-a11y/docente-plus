{
  "name": "docente-plus",
  "version": "1.0.0",
  "main": "docente-plus/App.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "expo": "54.0.12",
    "expo-document-picker": "~13.0.2",
    "expo-file-system": "~19.0.16",
    "expo-print": "^15.0.7",
    "expo-sharing": "~13.0.0",
    "expo-sqlite": "~16.0.8",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "babel-preset-expo": "~11.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "db/**/*.js",
      "!db/**/*.test.js",
      "!**/node_modules/**"
    ]
  },
  "private": true
}
